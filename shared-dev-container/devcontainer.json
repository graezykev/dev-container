{
  "name": "Dev Container",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "app",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "latest"
    },
    "ghcr.io/robbert229/devcontainer-features/postgresql-client:1": {}
  },
  "workspaceFolder": "/workspace",
  "note": "Dont set `workspaceFolder` for Codespaces; Or use `localEnv` to set `workspaceFolder`",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "terminal.integrated.defaultProfile.linux": "zsh"
      },
      "extensions": [
        "dbaeumer.vscode-eslint@3.0.5",
        "esbenp.prettier-vscode",
        "eamodio.gitlens",
        "github.copilot"
      ]
    }
  },
  "postCreateCommand": "echo 'Dev Container Built' && .devcontainer/post-create.sh",
  "updateContentCommand": "",
  "postStartCommand": "echo 'Dev Container Started'",
  "postAttachCommand": "echo 'Dev Container Attached'"
}